{
  "manifest_version": 3,
  "name": "Focus Feed",
  "version": "1.0.0",
  "description": "Stop algorithmic rot by replacing YouTube's feed with curated content",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["presets.js", "content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}
